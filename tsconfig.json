{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/api/send/route.ts", "app/components/MyContact.tsx", "actions/handleScroll.ts", "app/navbar/Navbar.jsx", "app/components/Introduction.jsx", "app/page.jsx", "lib/utils.ts", "app/components/AboutMe.jsx", "app/components/Project.jsx"],
  "exclude": ["node_modules"]
}
